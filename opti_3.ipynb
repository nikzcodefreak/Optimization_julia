    "using Convex\n",
    "x=Variable()\n",
    "y=Variable()\n",
    "z=maximize(8*x+5*y)\n",
    "z.constraints+=20*x+10*y<=5000\n",
    "z.constraints+=x<=150\n",
    "z.constraints+=y<=250\n",
    "z.constraints+=x>=0\n",
    "z.constraints+=y>=0\n",
    "solve!(z)"
   ]
