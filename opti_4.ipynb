    "using Convex\n",
    "x=Variable()\n",
    "y=Variable()\n",
    "z=minimize(5*x+4*y)\n",
    "z.constraints+=2*x+3*y>=90\n",
    "z.constraints+=4*x+y>=40\n",
    "z.constraints+=x>=0\n",
    "z.constraints+=y>=0\n",
    "solve!(z)"
